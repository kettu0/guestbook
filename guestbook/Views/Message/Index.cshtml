@model List<guestbook.Models.Message>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<ul>
    @foreach (var message in Model)
    {
        <li>@message.Nickname</li>
        <p>@message.UserMessage</p>
        <p>@message.eMail</p>
        <p>@message.CurrentDate</p>
    }
</ul>
